<?xml version="1.0" encoding="utf-8"?>
<AutoVisualizer xmlns="http://schemas.microsoft.com/vstudio/debugger/natvis/2010">

	<!-- Custom Visualizer for SharedPtr -->
	<Type Name="SharedPtr&lt;*&gt;">
		<DisplayString>{{ptr = {ptr}, refCount = {*refCount}}}</DisplayString>
		<Expand>
			<Item Name="Pointer">ptr</Item>
			<Item Name="Reference Count">*refCount</Item>
		</Expand>
	</Type>

</AutoVisualizer>
