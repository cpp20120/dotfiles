<?xml version="1.0" encoding="utf-8"?>
<AutoVisualizer xmlns="http://schemas.microsoft.com/vstudio/debugger/natvis/2010">

	<!-- Custom Visualizer for Queue -->
	<Type Name="Queue&lt;*&gt;">
		<DisplayString>{{size = {container.size()}}}</DisplayString>
		<Expand>
			<Item Name="Size">container.size()</Item>
			<ArrayItems>
				<Size>container.size()</Size>
				<ValuePointer>container._Myptr</ValuePointer>
			</ArrayItems>
		</Expand>
	</Type>

</AutoVisualizer>
